{
  "name": "imgpool",
  "version": "0.0.0",
  "description": "Lightweight image board",
  "main": "app.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "scripts": {
    "init": "node init.js",
    "server:dev": "NODE_ENV=development node server-build/server.js",
    "server:prod": "NODE_ENV=production node server-build/server.js",
    "build:client:dev": "webpack --env.environment=development && ln -s ../uploads ./public/uploads",
    "build:client:prod": "webpack --env.environment=production && ln -s ../uploads ./public/uploads",
    "build:server:dev": "webpack --env.environment=development --config webpack.server.js",
    "build:server:prod": "webpack --env.environment=production --config webpack.server.js",
    "test": "jest"
  },
  "author": "David Roffe <davidroffe@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.20.0",
    "eslint": "5.16.0",
    "koa": "^2.13.0",
    "koa-logger": "^3.2.1",
    "koa-send": "^5.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.0.5",
    "react-toastify": "^5.4.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.1.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.11.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "redux-mock-store": "^1.5.4",
    "sequelize-cli": "^5.5.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "**/**/minimist": "^1.2.3",
    "**/**/acorn": "^6.4.1"
  }
}
